   /*calculates the game in which the winning team should play */
   int getNextGame(int OriginalGameNumber) {
       double gameNumberDivided = (double) OriginalGameNumber / 2;
       int newNumber = (int)Math.ceil(gameNumberDivided);

       return newNumber + 32;
   }
